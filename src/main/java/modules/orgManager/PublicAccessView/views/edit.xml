<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="edit"
	title="PublicAccessView"
	xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
	<form responsiveWidth="12" percentageWidth="60" border="true">
		<column percentageWidth="30" responsiveWidth="4" />
		<column />
		<row>
			<item>
				<lookupDescription binding="selectedOffice"
					descriptionBinding="bizKey">
					<onPickedHandlers>
						<rerender></rerender>
					</onPickedHandlers>
				</lookupDescription>
			</item>
		</row>
	</form>
	<map modelName="OfficeMapModel" visible="showMap"></map>
	<newParameters />
</view>
