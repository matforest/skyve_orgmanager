<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Office" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="ORG_Office"/>
    <singularAlias>Office</singularAlias>
    <pluralAlias>Offices</pluralAlias>
    <iconStyleClass>fa fa-building</iconStyleClass>
    <bizKey expression="Office - {streetAddress1}"/>
    <attributes>
        <text name="levelUnit">
            <displayName>Level Unit</displayName>
            <length>200</length>
        </text>
        <text name="buildingName">
            <displayName>Building Name</displayName>
            <length>200</length>
        </text>
        <text name="streetAddress1">
            <displayName>Street Address 1</displayName>
            <length>200</length>
        </text>
        <text name="streetAddress2">
            <displayName>Street Address 2</displayName>
            <length>200</length>
        </text>
        <text name="suburb">
            <displayName>Suburb</displayName>
            <length>200</length>
        </text>
        <text name="postcode">
            <displayName>Postcode</displayName>
            <length>20</length>
            <format mask="####"/>
        </text>
        <text name="phone">
            <displayName>Phone</displayName>
            <length>14</length>
            <format mask="##############"/>
        </text>
    </attributes>
</document>